<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="android.com.pedrojose.rater.activities.EvalPreStart">

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Modo Query"
        android:drawableTop="@drawable/eval"
        android:id="@+id/button24"
        android:onClick="back2Main"
        android:textSize="20sp"
        android:fontFamily="droid"
        android:layout_above="@+id/button25"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/button25"
        android:layout_alignEnd="@+id/button25" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Mapa"
        android:drawableTop="@drawable/location"
        android:onClick="back2Main"
        android:textSize="20sp"
        android:fontFamily="droid"
        android:id="@+id/button25"
        android:layout_above="@+id/button26"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/button26"
        android:layout_alignEnd="@+id/button26" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Voltar"
        android:drawableTop="@drawable/left"
        android:onClick="back2Main"
        android:textSize="20sp"
        android:fontFamily="droid"
        android:id="@+id/button26"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Large Text"
        android:id="@+id/textView11"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

</RelativeLayout>
